{
	"compilerOptions": {
		"target": "ES2018",
		"module": "commonjs",
    "resolveJsonModule": true,
		"jsx": "react-jsx",
		"outDir": "./dist",
		"strict": true,
		"noEmit": true,
		"lib": ["es2015"],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
